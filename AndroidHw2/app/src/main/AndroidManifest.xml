<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.user.androidhw2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="com.example.user.androidhw2.LOGINACTIVITY" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".TimetableActivity" />
        <activity android:name=".TipCalculatorActivity" />
        <activity android:name=".MiniCalculatorActivity" />
        <activity android:name=".ChangePsActivity" />
        <activity android:name=".Description1" />
        <activity android:name=".Description2" />
        <activity android:name=".Description3"></activity>
    </application>

</manifest>
